    "# 1) WAP to check if an input number is odd or even\n",
    "x = input(\"Enter a number: \")\n",
    "def check(n):\n",
    "    if (n % 2 == 0):\n",
    "        print('Even Number')\n",
    "    else:\n",
    "        print('Odd Number')\n",
    "check(int(x))"
